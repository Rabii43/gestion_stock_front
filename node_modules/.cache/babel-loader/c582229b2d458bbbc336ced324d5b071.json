AA,EAAA,KANA;AAOA,MAAA,YAAA,EAAA,KAPA;AAQA,MAAA,cAAA,EAAA;AARA,KAAA;AAUA,GAxBA;AAyBA,EAAA,WAAA,EAAA;AACA,IAAA,QAAA,EAAA;AACA,MAAA,QAAA,EAAA;AADA,KADA;AAIA,IAAA,eAAA,EAAA;AACA,MAAA,QAAA,EAAA,QADA;AAEA,MAAA,SAAA,EAAA,SAAA,CAAA,CAAA,CAFA;AAGA,MAAA,cAAA,EAAA,MAAA,CAAA,UAAA;AAHA;AAJA,GAzBA;AAmCA,EAAA,OAAA,kCACA,WADA;AAEA;AACA;AACA,IAAA,UAJA,wBAIA;AAAA;;AACA,WAAA,SAAA,GAAA,IAAA,CADA,CAEA;;AACA,WAAA,EAAA,CAAA,MAAA;;AACA,UAAA,KAAA,EAAA,CAAA,QAAA,EAAA;AACA;AACA,OAFA,MAEA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,aAAA,GAAA,GAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,KAAA,EAAA;AACA,UAAA,QAAA,EAAA,KAAA;AADA,SAAA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,EAAA;AACA,gBAAA,GAAA,CAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,cAAA,KAAA,CAAA,cAAA,GAAA,IAAA;AACA,cAAA,KAAA,CAAA,YAAA,GAAA,KAAA;AACA,cAAA,KAAA,CAAA,YAAA,GAAA,+CAAA,CAHA,CAIA;;AACA,cAAA,UAAA,CAAA;AAAA,uBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,kBAAA,IAAA,EAAA;AAAA,iBAAA,CAAA;AAAA,eAAA,EAAA,IAAA,CAAA;AACA,aANA,MAMA;AACA,cAAA,KAAA,CAAA,YAAA,GAAA,IAAA;AACA,cAAA,KAAA,CAAA,cAAA,GAAA,KAAA;AACA,cAAA,KAAA,CAAA,KAAA,GAAA,kCAAA;AACA;AACA;AACA,SAhBA,EAgBA,KAhBA,CAgBA,UAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,YAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,cAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,KAAA,GAAA,2BAAA;AACA,SApBA;AAsBA;AACA;AAlCA;AAnCA,CAAA","sourcesContent":["<template>\n  <Layout>\n    <div class=\"row justify-content-center\">\n      <div class=\"col-md-8 col-lg-6 col-xl-5\">\n        <div class=\"card overflow-hidden\">\n          <div class=\"bg-card-header\">\n            <div class=\"row\">\n              <div class=\"col-7\">\n                <div class=\"text-primary p-4\">\n                  <h5 class=\"text-primary\">Bienvenue !</h5>\n                  <p>Changer votre mot de passe.</p>\n                </div>\n              </div>\n              <div class=\"col-5 align-self-end\">\n                <img\n                    src=\"@/assets/images/profile-img.png\"\n                    alt\n                    class=\"img-fluid\"\n                />\n              </div>\n            </div>\n          </div>\n          <div class=\"card-body pt-0\">\n            <div>\n              <router-link tag=\"a\" to=\"/\">\n                <div class=\"avatar-md profile-user-wid mb-4\">\n                  <span class=\"avatar-title rounded-circle bg-white-circle\">\n                    <img src=\"@/assets/images/logo.svg\" alt height=\"50\"/>\n                  </span>\n                </div>\n              </router-link>\n            </div>\n\n            <div class=\"p-2\">\n              <b-alert\n                  v-model=\"isResetSuccess\"\n                  class=\"mt-3\"\n                  variant=\"success\"\n                  dismissible\n              >{{ resetSuccess }}.\n              </b-alert>\n              <b-alert\n                  v-model=\"isResetError\"\n                  class=\"mb-4\"\n                  variant=\"danger\"\n                  dismissible\n              >{{ error }}\n              </b-alert>\n              <b-form class=\"p-2\" @submit.prevent=\"tryToReset\">\n                <b-form-group\n                    class=\"mb-3\"\n                    id=\"input-group-2\"\n                    label=\"Nouveau mot de passe *\"\n                    label-for=\"input-2\"\n                >\n                  <b-form-input\n                      id=\"input-2\"\n                      v-model=\"password\"\n                      type=\"password\"\n                      placeholder=\"Entrez votre mot de passe\"\n                      :class=\"{ 'is-invalid': submitted && $v.password.$error }\"\n                  ></b-form-input>\n                  <div\n                      v-if=\"submitted && !$v.password.required\"\n                      class=\"invalid-feedback\"\n                  >\n                    Mot de passe est obligatoire.\n                  </div>\n                </b-form-group>\n                <b-form-group\n                    class=\"mb-3\"\n                    id=\"password-group\"\n                    label=\"Confirmez votre mot de passe *\"\n                    label-for=\"password\"\n                >\n                  <b-form-input\n                      id=\"password\"\n                      v-model=\"confirmPassword\"\n                      type=\"password\"\n                      placeholder=\"Confirmez votre mot de passe\"\n                      :class=\"{\n                    'is-invalid': submitted && $v.confirmPassword.$error,\n                  }\"\n                  ></b-form-input>\n                  <div\n                      v-if=\"submitted && !$v.confirmPassword.required\"\n                      class=\"invalid-feedback\"\n                  >\n                    Mot de passe est obligatoire.\n                  </div>\n                  <div\n                      v-if=\"submitted && !$v.confirmPassword.sameAsPassword\"\n                      class=\"invalid-feedback\"\n                  >\n                    mot de passe non valide\n                  </div>\n                  <div\n                      v-if=\"submitted && !$v.confirmPassword.minLength\"\n                      class=\"invalid-feedback\"\n                  >\n                    La longueur du mot de passe doit être supérieure à 5\n                  </div>\n                </b-form-group>\n                <div class=\"mt-4 d-grid\">\n                  <b-button type=\"submit\" variant=\"magenta\" class=\"btn-block\">Valider\n                  </b-button>\n                </div>\n              </b-form>\n            </div>\n          </div>\n          <!-- end card-body -->\n        </div>\n        <!-- end card -->\n      </div>\n      <!-- end col -->\n    </div>\n    <!-- end row -->\n  </Layout>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nimport Layout from \"../../layouts/auth\";\nimport {authMethods} from \"@/state/helpers\";\nimport appConfig from \"@/app.config\";\n\nimport {minLength, required, sameAs} from \"vuelidate/lib/validators\";\nimport {resetPassword} from \"../../../../api/entryPoint\";\n\n/**\n * Forgot Password component\n */\nexport default {\n  page: {\n    title: \"New Password\",\n    meta: [\n      {\n        name: \"description\",\n        content: appConfig.description,\n      },\n    ],\n  },\n  components: {\n    Layout,\n  },\n  data() {\n    return {\n      password: \"\",\n      confirmPassword: \"\",\n      resetSuccess: \"\",\n      submitted: false,\n      error: null,\n      tryingToReset: false,\n      isResetError: false,\n      isResetSuccess: false,\n    };\n  },\n  validations: {\n    password: {\n      required,\n    },\n    confirmPassword: {\n      required,\n      minLength: minLength(6),\n      sameAsPassword: sameAs('password'),\n    },\n  },\n  methods: {\n    ...authMethods,\n    // Try to register the user in with the email, fullname\n    // and password they provided.\n    tryToReset() {\n      this.submitted = true;\n      // stop here if form is invalid\n      this.$v.$touch();\n      if (this.$v.$invalid) {\n        return;\n      } else {\n        axios.post(resetPassword + '/' + this.$route.params.token, {\n          password: this.password,\n        }).then((res) => {\n          if (res) {\n            if (res.data.code === 200) {\n              this.isResetSuccess = true;\n              this.isResetError = false;\n              this.resetSuccess = \"Votre mot de passe a été modifié avec succès.\";\n              // Redirect\n              setTimeout(() => this.$router.push({path: '/login'}), 2000);\n            } else {\n              this.isResetError = true;\n              this.isResetSuccess = false;\n              this.error = \"Votre mot de passe non modifier.\";\n            }\n          }\n        }).catch((err) => {\n              this.isResetError = true;\n              this.isResetSuccess = false;\n              this.error = \"Erreur de la modification\";\n            }\n        )\n      }\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/router/views/account"}]}