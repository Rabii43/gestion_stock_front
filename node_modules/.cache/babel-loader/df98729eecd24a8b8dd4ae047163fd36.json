AA,CAAA,MAAA;;AACA,UAAA,KAAA,EAAA,CAAA,QAAA,EAAA;AACA;AACA,OAFA,MAEA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA;AACA,UAAA,KAAA,EAAA,KAAA;AADA,SAAA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,EAAA;AACA,gBAAA,GAAA,CAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,cAAA,KAAA,CAAA,cAAA,GAAA,IAAA;AACA,cAAA,KAAA,CAAA,YAAA,GAAA,KAAA;AACA,cAAA,KAAA,CAAA,YAAA,GAAA,oCAAA;AACA,qBAAA,GAAA;AACA,aALA,MAKA;AACA,cAAA,KAAA,CAAA,YAAA,GAAA,IAAA;AACA,cAAA,KAAA,CAAA,cAAA,GAAA,KAAA;AACA,cAAA,KAAA,CAAA,KAAA,GAAA,qBAAA;AACA;AACA;AACA,SAfA,EAeA,KAfA,CAeA,UAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,YAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,cAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,KAAA,GAAA,qBAAA;AACA,SAnBA;AAoBA;AACA;AAhCA;AA9BA,CAAA","sourcesContent":["<script>\nimport axios from \"axios\";\n\nimport Layout from \"../../layouts/auth\";\nimport {authMethods} from \"@/state/helpers\";\nimport appConfig from \"@/app.config\";\n\nimport {required, email} from \"vuelidate/lib/validators\";\nimport {resetPassword} from \"../../../../api/entryPoint\";\n\n/**\n * Forgot Password component\n */\nexport default {\n  page: {\n    title: \"Reset Password\",\n    meta: [\n      {\n        name: \"description\",\n        content: appConfig.description,\n      },\n    ],\n  },\n  components: {\n    Layout,\n  },\n  data() {\n    return {\n      email: \"\",\n      resetSuccess: \"\",\n      submitted: false,\n      error: null,\n      tryingToReset: false,\n      isResetError: false,\n      isResetSuccess: false,\n    };\n  },\n  validations: {\n    email: {\n      required,\n      email,\n    },\n  },\n  methods: {\n    ...authMethods,\n    // Try to register the user in with the email, fullname\n    // and password they provided.\n    tryToReset() {\n      this.submitted = true;\n      // stop here if form is invalid\n      this.$v.$touch();\n      if (this.$v.$invalid) {\n        return;\n      } else {\n        axios.post(resetPassword, {\n          email: this.email,\n        }).then((res) => {\n          if (res) {\n            if (res.data.code === 200) {\n              this.isResetSuccess = true;\n              this.isResetError = false;\n              this.resetSuccess = \"L'e-mail a été envoyé avec succès.\";\n              return res;\n            } else {\n              this.isResetError = true;\n              this.isResetSuccess = false;\n              this.error = \"Courriel non valide\";\n            }\n          }\n        }).catch((err) => {\n          this.isResetError = true;\n          this.isResetSuccess = false;\n          this.error = \"erreur cortage test\";\n        })\n      }\n    },\n  },\n};\n</script>\n\n<template>\n  <Layout>\n    <div class=\"row justify-content-center\">\n      <div class=\"col-md-8 col-lg-6 col-xl-5\">\n        <div class=\"card overflow-hidden\">\n          <div class=\"bg-card-header\">\n            <div class=\"row\">\n              <div class=\"col-7\">\n                <div class=\"text-primary p-4\">\n                  <h5 class=\"text-primary\">Réinitialiser le mot de passe</h5>\n                  <p>Réinitialiser avec Fibourse.</p>\n                </div>\n              </div>\n              <div class=\"col-5 align-self-end\">\n                <img\n                    src=\"@/assets/images/profile-img.png\"\n                    alt\n                    class=\"img-fluid\"\n                />\n              </div>\n            </div>\n          </div>\n          <div class=\"card-body pt-0\">\n            <div>\n              <router-link tag=\"a\" to=\"/\">\n                <div class=\"avatar-md profile-user-wid mb-4\">\n                  <span class=\"avatar-title rounded-circle bg-white-circle\">\n                    <img src=\"@/assets/images/logo.svg\" alt height=\"50\"/>\n                  </span>\n                </div>\n              </router-link>\n            </div>\n\n            <div class=\"p-2\">\n              <b-alert\n                  v-model=\"isResetSuccess\"\n                  class=\"mt-3\"\n                  variant=\"success\"\n                  dismissible\n              >{{ resetSuccess }}.\n              </b-alert>\n              <b-alert\n                  v-model=\"isResetError\"\n                  class=\"mb-4\"\n                  variant=\"danger\"\n                  dismissible\n              >{{ error }}\n              </b-alert>\n              <form @submit.prevent=\"tryToReset\">\n                <div class=\"mb-3\">\n                  <label for=\"useremail\">Email</label>\n                  <input\n                      type=\"email\"\n                      v-model=\"email\"\n                      class=\"form-control\"\n                      id=\"useremail\"\n                      placeholder=\"Entrer votre Email\"\n                      :class=\"{ 'is-invalid': submitted && $v.email.$error }\"\n                  />\n                  <div\n                      v-if=\"submitted && $v.email.$error\"\n                      class=\"invalid-feedback\">\n                    <span v-if=\"!$v.email.required\">Email est requis.</span>\n                    <span v-if=\"!$v.email.email\"\n                    >Veuillez entrer une adresse email valide.</span>\n                  </div>\n                </div>\n                <div class=\"mt-4 mb-4 d-grid\">\n                  <div class=\"col-12 text-end\">\n                    <b-button type=\"submit\" variant=\"magenta\" class=\"btn-block\"\n                    >Réinitialiser\n                    </b-button>\n                  </div>\n                </div>\n              </form>\n            </div>\n          </div>\n          <!-- end card-body -->\n        </div>\n        <!-- end card -->\n\n        <div class=\"mt-5 text-center\">\n          <p>\n            Tu te souviens?\n            <router-link\n                tag=\"a\"\n                to=\"/login\"\n                class=\"fw-medium text-primary\"\n            >Se connecter\n            </router-link>\n          </p>\n        </div>\n      </div>\n      <!-- end col -->\n    </div>\n    <!-- end row -->\n  </Layout>\n</template>\n\n<style lang=\"scss\" module></style>\n"],"sourceRoot":"src/router/views/account"}]}