{
  "name": "bootstrap-vue-editable-table",
  "version": "0.1.1-beta.3",
  "description": "A Bootstrap Vue editable table for editing cells using built-in Bootstrap form elements",
  "repository": {
    "type": "git",
    "url": "https://github.com/muhimasri/b-editable-table"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/muhimasri/b-editable-table",
  "keywords": [
    "Vue",
    "Bootstrap",
    "Table",
    "Editable"
  ],
  "author": "Muhi Masri",
  "license": "MIT",
  "main": "dist/b-editable-table.ssr.js",
  "browser": "dist/b-editable-table.esm.js",
  "module": "dist/b-editable-table.esm.js",
  "unpkg": "dist/b-editable-table.min.js",
  "types": "b-editable-table.d.ts",
  "files": [
    "dist/*",
    "b-editable-table.d.ts",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "typescript": "^3.8.3",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.12",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.21.2"
  },
  "engines": {
    "node": ">=12"
  }
}
